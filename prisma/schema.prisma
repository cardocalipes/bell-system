generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:Bellsystem_2@localhost:3306/bellsystem"
}

model schedule1 {
  id         Int       @id
  ringnumber Int?
  timer      DateTime? @db.Time(0)
  realtime   DateTime? @db.Time(0)
}

model schedule2 {
  id         Int       @id
  ringnumber Int?
  timer      DateTime? @db.Time(0)
  realtime   DateTime? @db.Time(0)
}

model schedule3 {
  id         Int       @id
  ringnumber Int?
  timer      DateTime? @db.Time(0)
  realtime   DateTime? @db.Time(0)
}

model savedsched {
  id        Int       @id
  schedNum  Int?
  schedName String?   @db.VarChar(255)
  startTime DateTime? @db.Time(0)
  duration  Int?
  increment Int?
  breaktime Boolean?
}

model history {
  id      Int       @id
  date    DateTime? @db.DateTime(0)
  time    DateTime? @db.Time(0)
  type    String?   @db.VarChar(255)
  ringnum Int?
}
